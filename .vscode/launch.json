{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train lstm-dqn sample agent",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/",
            "program": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/train.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/data/SaladWorld/level_1.ulx",
                "${workspaceFolder}/data/SaladWorld/level_2.ulx"
            ]
        },
        {
            "name": "Test lstm-dqn sample agent",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/",
            "program": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/test.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Train lstm-dqn",
            "type": "python",
            "request": "launch",
            "program": "./conbas/agents/lstm_dqn/train.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/conbas/agents/lstm_dqn/config.local.yaml"
            ]
        },
        {
            "name": "Play",
            "type": "python",
            "request": "launch",
            "program": "./conbas/apps/play.py",
            "console": "integratedTerminal",
            "args": [
                "./data/SaladWorld/level_1.ulx",
                "./experiments/lstm_dqn/saved_models/model_weights_2.pt",
                "--max-steps=50"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "conbas.main"
        }
    ]
}
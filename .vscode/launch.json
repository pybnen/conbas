{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train lstm-dqn sample agent",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/",
            "program": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/train.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/data/SaladWorld/level_1.ulx",
                "${workspaceFolder}/data/SaladWorld/level_2.ulx"
            ]
        },
        {
            "name": "Test lstm-dqn sample agent",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/",
            "program": "${workspaceFolder}/FTWP/starting_kit/sample_submission_lstm-dqn/test.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Module: conbas.apps.play",
            "type": "python",
            "request": "launch",
            "module": "conbas.apps.play",
            "console": "integratedTerminal",
            "args": [
                "./data/SaladWorld/level_1.ulx",
                "./experiments/lstm_dqn/saved_models/model_weights_7.pt",
                "--max-steps=50"
            ]
        },
        {
            "name": "Module: conbas.apps.eval",
            "type": "python",
            "request": "launch",
            "module": "conbas.apps.eval",
            "args": [
                "./data/SaladWorld/level_1.ulx",
                "--agent=lstm_dqn",
                "--ckpt-path=./experiments/lstm_dqn/saved_models/model_weights_7.pt",
                "--max-steps=50"
            ]
        },
        {
            "name": "Module: conbas.agents.lstm_dqn.train",
            "type": "python",
            "request": "launch",
            "module": "conbas.agents.lstm_dqn.train",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/conbas/agents/lstm_dqn/config.local.yaml"
            ]
        },
    ]
}